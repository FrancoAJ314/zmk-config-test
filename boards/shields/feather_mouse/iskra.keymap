#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp 1 &kp 2 &kp 3  /* row 0, cols 0-2 */
                &kp 4 &kp 5 &kp 6  /* row 1, cols 0-2 */
                &kp 7 &kp 8 &kp 9  /* row 2, cols 0-2 */
                &kp A &kp 0 &kp B  /* row 3, cols 0-2 */
            >;
        };
    };
};
